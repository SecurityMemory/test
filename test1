Public Function xggtug(ByVal sData As String) As String
    Dim i       As Long
    Dim result  As String
    Dim temp    As Integer
    
    result = "" ' ????????????? ?? ?????????
    
    For i = 1 To Len(sData)
        temp = Asc(Mid$(sData, i, 1)) Xor 221 ' ??????? ???????? XOR ? ????? 123
        result = result & Chr$(temp And 255) ' ????????????? ????????? ??????? ? ?????? ? ?? ???????? ??? ?????????, ???? ??????????? ????????? 0-255
    Next i
    
    xggtug = result
End Function
